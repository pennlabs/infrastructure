          cache-from: type=local,src=/tmp/.buildx-cache,type=registry,ref=pennlabs/example-backend:latest
          cache-to: type=local,dest=/tmp/.buildx-cache
          tags: pennlabs/example-backend:latest,pennlabs/example-backend:\${{ github.sha }}
          outputs: type=docker,dest=/tmp/image.tar
      - uses: actions/upload-artifact@v2
        with:
          name: build-backend
          path: /tmp/image.tar
    needs: django-check
  react-check:
    name: React Check
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: Cache
        uses: actions/cache@v2
        with:
          path: \\"**/node_modules\\"
          key: v0-\${{ hashFiles('frontend/yarn.lock') }}
      - name: Install Dependencies
        run: |-
