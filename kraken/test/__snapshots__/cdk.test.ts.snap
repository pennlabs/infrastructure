exports[`default 1`] = `
"# Generated by cdkactions. Do not modify
# Generated as part of the 'example' stack.
name: Publish example
on:
  push:
    paths:
      - example/**
jobs:
  publish:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: Cache
        uses: actions/cache@v2
        with:
          path: '**/node_modules'
          key: v0-\${{ hashFiles('example/yarn.lock') }}
      - name: Install Dependencies
        run: |-
          cd example
          yarn projen
      - name: Test
        run: |-
          cd example
          yarn test
      - name: Upload Code Coverage
        run: |-
          ROOT=$(pwd)
          cd example
          yarn run codecov -p $ROOT -F example
      - name: Publish to npm
        run: |-
          cd example
          yarn compile
          yarn package
          mv dist/js/*.tgz dist/js/kraken.tgz
          yarn publish --non-interactive --access public dist/js/kraken.tgz
        if: github.ref == 'refs/heads/master'
        env:
          NPM_AUTH_TOKEN: \${{ secrets.NPM_AUTH_TOKEN }}
    container:
      image: 'node:14'
"
`;
