{
  "name": "pennlabs-actions-pypi",
  "version": "0.1.0",
  "private": false,
  "description": "Penn Labs PyPI action",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -s src/main.ts -o dist",
    "all": "yarn build && yarn format && yarn lint && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pennlabs/infrastructure.git"
  },
  "keywords": [
    "actions",
    "pennlabs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5"
  },
  "devDependencies": {
    "@types/node": "^14.10.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.8.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "typescript": "^4.0.2"
  }
}
